void setup(){
  //put your setup code here,to run once:
  serial1.begin(9600);
  PinMode(21,OUTPUT);
  PinMode(20,OUTPUT);
  PinMode(19,OUTPUT);
}
void loop(){
  //put your main code here,to run repeatedly:
  digitalWrite(21,HIGH);
  delay(3000);
  //This speeds up the simulation
  digitalWrite(21,LOW);
  digitalWrite(21,HIGH);
  delay(3000);
  digitalWrite(20,LOW);
  digitalWrite(19,HIGH);
  delay(3000);
  digitalWrite(19,LOW);
}
